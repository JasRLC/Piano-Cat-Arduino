int third;
int left;
int right;


#define NOTE_C 262
#define NOTE_D 294
#define NOTE_E 330
#define NOTE_F 349
#define NOTE_G 392
#define NOTE_A 440
#define NOTE_B 493

void setup() 
{
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C); 
  display.clearDisplay();                  
  delay(2000);                                 
  pinMode(LEFTBUTTON, INPUT_PULLUP);         
  pinMode(RIGHTBUTTON, INPUT_PULLUP);          
  pinMode(THIRDBUTTON, INPUT_PULLUP);
  pinMode(BUZZER_PIN, OUTPUT);                
}

void loop() 
{
  left = digitalRead(LEFTBUTTON);
  right = digitalRead(RIGHTBUTTON);
  third = digitalRead(THIRDBUTTON); 



  if (left == LOW && right == HIGH && third == HIGH) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame1, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_G);  
    delay(50);
    
  } else if (left == HIGH && right == LOW && third == HIGH) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame2, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_A); 
    delay(50);
    
  } else if (left == LOW && right == LOW && third == HIGH) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame3, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_F); 
    delay(50);
    
  } else if (left == HIGH && right == HIGH && third == LOW) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame4, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_B);  
    delay(50);
    
  } else if (left == HIGH && right == LOW && third == LOW) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame5, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_D);  
    delay(50);

   } else if (left == LOW && right == HIGH && third == LOW) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame6, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_E); 
    delay(50);

   } else if (left == LOW && right == LOW && third == LOW) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame7, 128, 80, 1);
    display.display();
    tone(BUZZER_PIN, NOTE_F); 
    delay(50);

   } else if (left == HIGH && right == HIGH && third == HIGH) {
    display.clearDisplay();
    display.drawBitmap(0, 0, frame8, 128, 80, 1);
    display.display();
    noTone(BUZZER_PIN);
    delay(50);
  }
}
